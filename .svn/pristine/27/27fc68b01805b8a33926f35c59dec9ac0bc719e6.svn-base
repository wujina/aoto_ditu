<%@ page language="java" contentType="text/html; charset=utf-8"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags"%>
<sec:authentication property="principal" var="currentUser" />
  <form class="form-grid">
    <div class="title">
      <fmt:message key="user.info" />
    </div>
    <div class="form-group">
      <div class="col-2 control-label">
        <label for="username"><fmt:message key="user.username" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="username" readonly />
      </div>
      <div class="col-1"></div>
      <div class="col-2 control-label">
        <label for="realName"><fmt:message key="user.realName" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="realName" readonly />
      </div>
      <div class="col-1"></div>
    </div>
    <div class="form-group">
      <div class="col-2 control-label">
        <label for="orgName"><fmt:message key="org.orgName" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="orgName" readonly />
      </div>
      <div class="col-1"></div>
      <div class="col-2 control-label">
        <label for="phone"><fmt:message key="user.phone" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="phone" readonly />
      </div>
      <div class="col-1"></div>
    </div>
    <div class="form-group">
      <div class="col-2 control-label">
        <label for="mobile"><fmt:message key="user.mobile" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="mobile" readonly />
      </div>
      <div class="col-1"></div>
      <div class="col-2 control-label">
        <label for="email"><fmt:message key="user.email" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="email" readonly />
      </div>
      <div class="col-1"></div>
    </div>
    <div class="form-group">
      <div class="col-2 control-label">
        <label for="lastLoginIp"><fmt:message key="user.lastLoginIp" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="lastLoginIp" readonly />
      </div>
      <div class="col-1"></div>
      <div class="col-2 control-label">
        <label for="lastLoginDate"><fmt:message key="user.lastLoginDate" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="lastLoginDate" readonly />
      </div>
      <div class="col-1"></div>
    </div>
    <div class="form-group">
      <div class="col-2 control-label">
        <label for="createdByName"><fmt:message key="createdBy" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="createdByName" readonly />
      </div>
      <div class="col-1"></div>
      <div class="col-2 control-label">
        <label for="createdDate"><fmt:message key="createdDate" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="createdDate" readonly />
      </div>
      <div class="col-1"></div>
    </div>
    <div class="form-group">
      <div class="col-2 control-label">
        <label for="lastUpdatedByName"><fmt:message key="lastUpdatedBy" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="lastUpdatedByName" readonly />
      </div>
      <div class="col-1"></div>
      <div class="col-2 control-label">
        <label for="lastUpdatedDate"><fmt:message key="lastUpdatedDate" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-3">
        <input class="form-control" name="lastUpdatedDate" readonly />
      </div>
      <div class="col-1"></div>
    </div>
    <div class="form-group">
      <div class="col-2 control-label">
        <label for="address"><fmt:message key="user.address" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-9">
        <textarea class="form-control" name="address" style="height:35px;" readonly></textarea>
      </div>
      <div class="col-1"></div>
    </div>
    <div class="form-group">
      <div class="col-2 control-label">
        <label for="remark"><fmt:message key="user.remark" /> <fmt:message key="colon" /></label>
      </div>
      <div class="col-9">
        <textarea class="form-control" name="remark" style="height:35px;" readonly></textarea>
      </div>
      <div class="col-1"></div>
    </div>
  </form>